<%= form_for :album, url: albums_path do |f|%>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :file_name %><br>
    <%= f.text_field :file_name %>
  </p>
  
  <p>
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </p>

  <p>
    <%= f.label :location_id %><br>
    <%= f.text_field :location_id %>
  </p>

  <p>
    <%= f.label :user_id %><br>
    <%= f.text_field :user_id %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>